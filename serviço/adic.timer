[Unit]
Description=Agendamento para adicionar usuários ao Domínio

[Timer]
#Execute job if it missed a run due to machine being off
Persistent=true
#Run 120 seconds after boot for the first time
OnBootSec=120
Unit=adic.service
#OnCalendar=* -* -* 18:00:00
OnCalendar=*:0/5

[Install]
WantedBy=timers.target
